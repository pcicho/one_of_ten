{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "settings": {
        "settings": { "import/resolver": {"node": {"extensions": [".js", ".jsx", ".ts", ".tsx"]}} }
    },
    "rules": {
        "max-len": 0,
        "react/no-danger": 0,
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/prop-types": 0,
        "react/no-array-index-key": 0
    }
}